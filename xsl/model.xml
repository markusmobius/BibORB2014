<entrylist>
  <entry type="article">
    <required>
      <id/><author/><title/><journal/><year/>
    </required>
    <optional>
      <volume/><number/><pages/><month/><note/>
    </optional>
    <additional>
      <own/><read/><groups/><abstract/><keywords/>
      <url/><ad_url/><urlzip/><ad_urlzip/><pdf/><ad_pdf/>
      <website/><link/><longnotes/>
    </additional>
  </entry>
  
  <entry type="book">
    <required>
      <id/>
      <exalternative>
	<author/><editor/>
      </exalternative>
      <title/><publisher/><year/>
    </required>
    <optional>
      <exalternative>
        <volume/><number/>
      </exalternative>
      <series/><address/><edition/><month/><note/>
    </optional>
    <additional>
      <own/><read/><groups/><abstract/><keywords/>
      <url/><ad_url/><urlzip/><ad_urlzip/><pdf/><ad_pdf/>
      <website/><link/><longnotes/>
    </additional>
  </entry>
  
  <entry type="booklet">
    <required>
        <id/><title/>
    </required>
    <optional>
      <author/><howpublished/><address/><month/><year/><note/>
    </optional>
    <additional>
      <own/><read/><groups/><abstract/><keywords/>
      <url/><ad_url/><urlzip/><ad_urlzip/><pdf/><ad_pdf/>
      <website/><link/><longnotes/>
    </additional>
  </entry>

  <entry type="inbook">
