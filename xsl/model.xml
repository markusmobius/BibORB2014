      <exalternative>
        <author/><editor/>
      </exalternative>
      <title/><chapter/><pages/><publisher/><year/>
    </required>
    <optional>
      <alternative>
        <volume/><number/>
      </alternative>
      <series/><type/><address/><edition/><month/><note/>
    </optional>
    <additional>
      <own/><read/><groups/><abstract/><keywords/>
      <url/><ad_url/><urlzip/><ad_urlzip/><pdf/><ad_pdf/>
      <website/><link/><longnotes/>
    </additional>
  </entry>

